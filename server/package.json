{
  "name": "mern-chat-api",
  "version": "1.0.0",
  "description": "API for MERN Chat App",
  "main": "index.js",
  "scripts": {
    "ts": "tsc --noEmit",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "start": "ts-node -r tsconfig-paths/register src",
    "dev": "nodemon -q -w './**' --ext 'ts,json' --exec 'yarn start'",
    "pretty": "prettier --write .",
    "mongo": "cd mongo && docker-compose up -d"
  },
  "keywords": [
    "api",
    "express",
    "typescript",
    "socket.io",
    "mongodb"
  ],
  "author": "floatrx <ifloatrx@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.2",
    "socket.io": "^4.7.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.11.30",
    "@types/socket.io": "^3.0.2",
    "nodemon": "^3.1.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3"
  }
}
